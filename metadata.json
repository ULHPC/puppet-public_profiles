      "version_range": ">= 1.0.0"
    },
    {
      "name": "saz/motd",
      "version_requirement": "2.x"
    },
    {
      "name": "saz/ssh",
      "version_requirement": "2.x"
    },
    {
      "name": "puppetlabs/ntp",
